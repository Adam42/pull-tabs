{
    "manifest_version": 2,

    "name": "Save Tabs As",
    "description": "Save open tabs as files and other formats maybe",
    "version": "0.6",

    "permissions": [
        "tabs",
        "downloads",
        "identity",
        "http://*/*",
        "https://*/*",
        "https://getpocket.com/v3/*",
    ],

    "web_accessible_resources": [
        "chrome-extension://jhmcfppppedffaabfckmcgokchccekgn/pocket/*"
    ],

    "browser_action": {
        "default_icon": {
            "19": "icon-19.png",
            "38": "icon-38.png"
        },
        "default_popup": "popup.html",
        "default_title": "Save Tabs As"
    }
}
