{
    "manifest_version": 3,

    "name": "Pull Tabs",
    "description":
        "Save open tabs to disk as files or put them in your Pocket.",
    "version": "0.17.0",
    "web_accessible_resources": [{
        "resources": ["pocket.html", "https://getpocket.com/v3/*"],
        "matches": [],
        "extension_ids": []
    }],
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
     "host_permissions": [
            "https://getpocket.com/v3/*"
    ],